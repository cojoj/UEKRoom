<?xml version='1.0' encoding='utf-8'?>
<widget id="pl.krakow.uek.dev.uekroom" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="ActivityIndicator">
        <param name="ios-package" value="ActivityIndicator" />
    </feature>
    <splash height="480" src="www/img/splash.png" width="320" />
    <splash height="960" src="www/img/splash.png" width="640" />
    <splash height="1024" src="www/img/splash.png" width="768" />
    <splash height="2048" src="www/img/splash.png" width="1536" />
    <splash height="768" src="www/img/splash.png" width="1024" />
    <splash height="1536" src="www/img/splash.png" width="2048" />
    <splash height="1136" src="www/img/splash.png" width="640" />
    <splash height="1334" src="www/img/splash.png" width="750" />
    <splash height="2208" src="www/img/splash.png" width="1242" />
    <splash height="1242" src="www/img/splash.png" width="2208" />
    <name>UEKRoom</name>
    <description>
        Application for checking room availability at the Cracow University of Economics.
    </description>
    <author href="dev.uek.krakow.pl">Maciej Komorowski</author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="www/img/icon.png" />
</widget>
